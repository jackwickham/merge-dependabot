{
  "name": "merge-dependabot",
  "version": "1.0.0",
  "private": true,
  "description": "Github action that merges dependabot PRs automatically",
  "main": "lib/main.js",
  "scripts": {
    "start": "node --async-stack-traces ./lib/index.js",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackwickham/merge-dependabot.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "Jack Wickham",
  "license": "MIT",
  "dependencies": {
    "probot": "^11.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "nock": "^13.0.11",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
