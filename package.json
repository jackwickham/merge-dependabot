{
  "name": "merge-dependabot",
  "version": "1.0.0",
  "private": true,
  "description": "Github action that merges dependabot PRs automatically",
  "main": "lib/main.js",
  "scripts": {
    "start": "node --async-stack-traces ./lib/index.js",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackwickham/merge-dependabot.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "Jack Wickham",
  "license": "MIT",
  "dependencies": {
    "probot": "^12.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.0.5",
    "jest": "^26.6.3",
    "jest-circus": "^27.2.5",
    "nock": "^13.1.3",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
