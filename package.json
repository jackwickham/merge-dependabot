{
  "name": "merge-dependabot",
  "version": "1.0.0",
  "private": true,
  "description": "Github action that merges dependabot PRs automatically",
  "main": "lib/main.js",
  "scripts": {
    "start": "node --async-stack-traces ./lib/index.js",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackwickham/merge-dependabot.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "Jack Wickham",
  "license": "MIT",
  "dependencies": {
    "probot": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.0.23",
    "@typescript-eslint/parser": "^3.6.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^4.1.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "nock": "^13.0.11",
    "prettier": "^1.19.1",
    "ts-jest": "^26.5.4",
    "typescript": "^3.9.7"
  }
}
